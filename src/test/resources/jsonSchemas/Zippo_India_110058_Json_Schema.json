{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "post code",
    "country",
    "country abbreviation",
    "places"
  ],
  "properties": {
    "post code": {
      "$id": "#/properties/post code",
      "type": "string",
      "title": "The Post code Schema",
      "default": "",
      "examples": [
        "110058"
      ],
      "pattern": "^(.*)$"
    },
    "country": {
      "$id": "#/properties/country",
      "type": "string",
      "title": "The Country Schema",
      "default": "",
      "examples": [
        "India"
      ],
      "pattern": "^(.*)$"
    },
    "country abbreviation": {
      "$id": "#/properties/country abbreviation",
      "type": "string",
      "title": "The Country abbreviation Schema",
      "default": "",
      "examples": [
        "IN"
      ],
      "pattern": "^(.*)$"
    },
    "places": {
      "$id": "#/properties/places",
      "type": "array",
      "title": "The Places Schema",
      "items": {
        "$id": "#/properties/places/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "place name",
          "longitude",
          "state",
          "state abbreviation",
          "latitude"
        ],
        "properties": {
          "place name": {
            "$id": "#/properties/places/items/properties/place name",
            "type": "string",
            "title": "The Place name Schema",
            "default": "",
            "examples": [
              "Janakpuri"
            ],
            "pattern": "^(.*)$"
          },
          "longitude": {
            "$id": "#/properties/places/items/properties/longitude",
            "type": "string",
            "title": "The Longitude Schema",
            "default": "",
            "examples": [
              "77.2167"
            ],
            "pattern": "^(.*)$"
          },
          "state": {
            "$id": "#/properties/places/items/properties/state",
            "type": "string",
            "title": "The State Schema",
            "default": "",
            "examples": [
              "New Delhi"
            ],
            "pattern": "^(.*)$"
          },
          "state abbreviation": {
            "$id": "#/properties/places/items/properties/state abbreviation",
            "type": "string",
            "title": "The State abbreviation Schema",
            "default": "",
            "examples": [
              "DL"
            ],
            "pattern": "^(.*)$"
          },
          "latitude": {
            "$id": "#/properties/places/items/properties/latitude",
            "type": "string",
            "title": "The Latitude Schema",
            "default": "",
            "examples": [
              "28.6167"
            ],
            "pattern": "^(.*)$"
          }
        }
      }
    }
  }
}